<?xml version="1.0" encoding="UTF-8" ?>
<translation>
    <sentence key="CHANGEPASSWORD">
        <translation language="fr">
        Changement du mot de passe
        </translation>
        <translation language="en">
        Changing your password
        </translation>
    </sentence>
    <sentence key="CHANGEPASSWORD_DESCRIPTION">
        <translation language="fr">
        Cette page vous permet de changer votre mot de passe. Entrez ci-dessous le nouveau mot de passe que vous souhaitez affecter à votre compte.
        </translation>
        <translation language="en">
        Change your password using the following form. Enter twice the new password you want to use.
        </translation>
    </sentence>
    <sentence key="CURRENTPASSWORD">
        <translation language="fr">
        Mot de passe actuel
        </translation>
        <translation language="en">
        Current password
        </translation>
    </sentence>
    <sentence key="NEWPASSWORD1">
        <translation language="fr">
        Nouveau mot de passe souhaité
        </translation>
        <translation language="en">
        Type your new password
        </translation>
    </sentence>
    <sentence key="NEWPASSWORD2">
        <translation language="fr">
        Entrer une seconde fois le nouveau mot de passe
        </translation>
        <translation language="en">
        Type again your new password
        </translation>
    </sentence>
    <sentence key="CHANGE_PASSWORD">
        <translation language="fr">
        Changer mon mot de passe
        </translation>
        <translation language="en">
        Change my password
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_ERROR">
        <translation language="fr">
        Une erreur s'est produite. Votre mot de passe n'a pas été changé.
        </translation>
        <translation language="en">
        An error occured. Your password has not been changed.
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_SUCCESS">
        <translation language="fr">
        Votre mot de passe a été changé.
        </translation>
        <translation language="en">
        You password has been changed.
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_ERROR_MINLENGTH">
        <translation language="fr">
        Votre mot de passe est trop court
        </translation>
        <translation language="en">
        Your password is too short
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_ERROR_DONTMATCH">
        <translation language="fr">
        Vos (nouveaux) mots de passe sont différents
        </translation>
        <translation language="en">
        Your passwords (new) don't match
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_ERROR_UNKNOWN">
        <translation language="fr">
        Une erreur inconnue est survenue
        </translation>
        <translation language="en">
        An unknown error occured
        </translation>
    </sentence>
    <sentence key="PASSWORDCHANGED_ERROR_WRONGPASSWORD">
        <translation language="fr">
        Vous n'avez pas saisi votre mot de passe actuel correctement
        </translation>
        <translation language="en">
        Your current password you typed is not correct
        </translation>
    </sentence>
</translation>